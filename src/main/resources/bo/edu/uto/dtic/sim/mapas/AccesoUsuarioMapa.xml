<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="bo.edu.uto.dtic.sim.mapas.AccesoUsuarioMapa">

	<select id="getAccesosUsuario" resultType="AccesoUsuario" parameterType="Usuarios" >
		select *
		from dea.unidad_usuario uu
		where uu.id_usuario = #{id_usuario}
	</select>

</mapper>
